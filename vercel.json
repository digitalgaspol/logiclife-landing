{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/__/auth/(.*)",
      "dest": "https://cuan-app-database.firebaseapp.com/__/auth/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
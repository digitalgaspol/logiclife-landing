{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}

{
  "rewrites": [
    {
      "source": "/__/auth/:match*",
      "destination": "https://cuan-app-database.firebaseapp.com/__/auth/:match*"
    }
  ]
}